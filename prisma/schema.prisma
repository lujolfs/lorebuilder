generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model lineages {
  id          Int      @id @default(autoincrement())
  name        String   @db.VarChar
  maxAge      Int
  adultAge    Int
  isPlayable  Boolean? @default(false)
  description String?
}
